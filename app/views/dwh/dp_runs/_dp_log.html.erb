<% decorate dp_log do |decorates| %>
  <li class="bg-white mb-1 px-4 py-2 shadow-sm sm:rounded-lg flex items-center justify-between gap-x-6 border-l-8 <%= decorates.status %> border-t border-r border-b border-gray-300">
    <div class="text-sm font-semibold leading-6 text-gray-700"><%= dp_log.message %></div>
  </li>
<% end %>