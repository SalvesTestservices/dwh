<div>
  <%= render "shared/section_header", title: I18n.t('.dg_quality_log.titles.index'), icon: "certificate" %>

  <ul role="list" class="divide-y divide-gray-300 bg-white shadow-sm ring-1 ring-gray-900/5 rounded-lg border border-gray-100">
    <% dg_quality_logs.each do |dg_quality_log| %>
      <% decorate dg_quality_log do |decorates| %>
        <li class="flex items-center justify-between p-3">
          <div class="flex flex-row">
            <div class="w-24 flex flex-row justify-center"><%= decorates.result%></div>
            <div class="text-base leading-6 text-gray-900 space pt-0.5">
              <%= decorates.data_field(dg_quality_log.error_message) %>
            </div>
          </div>
          <div class="flex flex-none items-center gap-x-1">
          </div>
        </li>
      <% end %>
    <% end %>
  </ul>
</div>