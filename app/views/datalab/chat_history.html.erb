<%= render "layouts/page_header", 
  breadcrumbs: @breadcrumbs, 
  title: I18n.t('.datalab.titles.chat_history'), 
  icon: ["history", "medium", "gray"],
  buttons: nil
%>

<main>
  <div class="mx-auto max-w-8xl py-2 md:py-6">
    <div id="chats" class="py-4">
      <% @chat_history.each do |chat| %>
        <%= render partial: "chat", locals: { chat: chat, show_more: true } %>
      <% end %>
    </div>
  </div>
</main>